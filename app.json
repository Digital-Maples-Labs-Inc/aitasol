{
  "expo": {
    "name": "Aitasol Education Consultancy",
    "slug": "aitasol-education-consultancy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "webpack",
      "output": "static",
      "build": {
        "babel": {
          "include": [
            "nativewind/babel"
          ]
        }
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "aitasol"
  }
}

